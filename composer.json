{
    "name": "upmind/provision-provider-software-licenses",
    "description": "Provision category for various providers of software licenses",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Harry Lewis",
            "email": "harry@upmind.com"
        },
        {
            "name": "Ana Carrasco",
            "email": "ana.carrasco@upmind.com"
        },
        {
            "name": "Roussetos Karafyllakis (RoussKS)",
            "email": "roussetos@upmind.com"
        }
    ],
    "require": {
        "php": "^7.4 || 8.1 - 8.3",
        "upmind/provision-provider-base": "^3.7 || ^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9 || ^10 || ^11",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "Upmind\\ProvisionProviders\\SoftwareLicenses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Upmind\\ProvisionProviders\\SoftwareLicenses\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Upmind\\ProvisionProviders\\SoftwareLicenses\\LaravelServiceProvider"
            ]
        }
    }
}
